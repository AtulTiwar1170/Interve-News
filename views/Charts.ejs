                        src="https://s3.tradingview.com/external-embedding/embed-widget-timeline.js" async>
                            {
                                "feedMode": "all_symbols",
                                    "isTransparent": false,
                                        "displayMode": "regular",
                                            "width": "450",
                                                "height": "1200",
                                                    "colorTheme": "light",
                                                        "locale": "en"
                            }
                        </script>
                </div>
                <!-- TradingView Widget END -->
            </div>
        </section>
